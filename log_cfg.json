{
    "version": 1,
    "handlers": {
        "standard": {
            "class": "logging.StreamHandler",
            "level": "DEBUG",
            "formatter": "standard",
            "filters": ["context_filter", "neon_logging_filter"]
        },
        "root": {
            "class": "logging.StreamHandler",
            "level": "DEBUG",
            "formatter": "root"
        }
    },
    "filters": {
        "context_filter": {
            "()": "logged_groups.ContextFilter"
        },
        "neon_logging_filter": {
            "()": "proxy.common_neon.logging.NeonPrefixDropper"
        }
    },
    "formatters": {
        "standard": {
            "format": "%(asctime)s.%(msecs)03d %(levelname)8s %(process)6d:%(threadName)-11s %(name)15s:%(class)-30s %(context)s %(message)s",
            "style": "%",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        },
        "root": {
            "format": "%(asctime)s.%(msecs)03d %(levelname)8s %(process)6d:%(threadName)-11s %(name)15s:Undefined                      %(message)s",
            "style": "%",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        }
    },
    "loggers": {
        "neon": {
            "level": "DEBUG",
            "handlers": ["standard"],
            "propagate": false
        },
        "": {
            "level": "DEBUG",
            "handlers": ["root"],
            "propagate": false
        }
    }
}
